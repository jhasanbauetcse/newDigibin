<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@color/softgreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".shareapp">

    <ImageView
        android:id="@+id/headerImage23"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/box_green" />

    <ImageView
        android:id="@+id/logoImage24"
        android:layout_width="77dp"
        android:layout_height="67dp"
        app:layout_constraintBottom_toBottomOf="@+id/headerImage23"
        app:layout_constraintEnd_toEndOf="@+id/headerImage23"
        app:layout_constraintHorizontal_bias="0.338"
        app:layout_constraintStart_toStartOf="@+id/headerImage23"
        app:layout_constraintTop_toTopOf="@+id/headerImage23"
        app:srcCompat="@drawable/mylogo" />
    <ImageView
        android:id="@+id/digiBinLogoImage25"
        android:layout_width="142dp"
        android:layout_height="59dp"
        app:layout_constraintBottom_toBottomOf="@+id/headerImage23"
        app:layout_constraintEnd_toEndOf="@+id/headerImage23"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toEndOf="@+id/logoImage24"
        app:layout_constraintTop_toTopOf="@+id/headerImage23"
        app:layout_constraintVertical_bias="0.6"
        app:srcCompat="@drawable/colldigibin" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share Link"
        android:textSize="24dp"
        android:textColor="@color/black"
        android:textStyle="bold"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headerImage23" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="Link:"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView44" />

    <TextView
        android:id="@+id/textView4756"
        android:layout_width="306dp"
        android:layout_height="68dp"
        android:layout_marginTop="24dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:text="https://www.google.com/search?q=digibinrlz=1C1BNSD_enBD107"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/textView45"
        app:layout_constraintTop_toBottomOf="@+id/textView44" />
</androidx.constraintlayout.widget.ConstraintLayout>